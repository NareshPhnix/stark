.c-checklist__column.c-checklist__priority

  if item.priority == "High"
    +svg-icon.icon.icon--small.icon-priority--high(aria-label="High priority item" data-icon-name='high-priority')

  else if item.priority == "Medium"
    +svg-icon.icon.icon--small.icon-priority--medium(aria-label="Medium priority item" data-icon-name='medium-priority')

  else
    +svg-icon.icon.icon--small.icon-priority--low(aria-label="Low priority item", data-icon-name='low-priority')
